{
	"name": "Replacer",
	"version":"0.1",
	"description":"An extension to replace text in medium article",
	"manifest_version": 2,
	"page_action":{
		"default_icon":{
			
			"16":"images/replacely16.png",
			"32": "images/replacely32.png",
            		"48": "images/replacely48.png"
		}
	},

	"icons": {
        	"16": "images/replacely16.png",
        	"32": "images/replacely32.png",
        	"48": "images/replacely48.png"
    },

    "content_scripts": [
	    {
	    "matches":["https://medium.com"],
	    "js": ["contentScript.js"]
	}
    ]
}
